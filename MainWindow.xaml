<Window x:Name="test" x:Class="Spider_Man_Material_Tool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:adonisExtensions="clr-namespace:AdonisUI.Extensions;assembly=AdonisUI"
        xmlns:local="clr-namespace:Spider_Man_Material_Tool"
        mc:Ignorable="d"
        Title="Spider-Man Material Tool" Height="800" Width="1500">
    <Grid Background="#FF2D2D2D">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="77*"/>
            <ColumnDefinition Width="73*"/>
        </Grid.ColumnDefinitions>
        <Label x:Name="title_text" HorizontalAlignment="Left" Margin="30,37,0,0" VerticalAlignment="Top" Height="67" Width="552" FontSize="45" FontFamily="Inter SemiBold" FontWeight="Normal" Background="{x:Null}">
            <Label.Foreground>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FFFF8585" Offset="0.5"/>
                    <GradientStop Color="#FF504AFF" Offset="0.537"/>
                </LinearGradientBrush>
            </Label.Foreground> Spider-Man Material Tool
        </Label>
        <Separator HorizontalAlignment="Left" Margin="55,92,0,0" VerticalAlignment="Top" Height="58" Width="324" Background="#FFFF6262"/>
        <Label x:Name="mg_text" Content="Material Values" HorizontalAlignment="Left" Margin="55,150,0,0" VerticalAlignment="Top" Foreground="White" Height="43" Width="266" FontFamily="Gantari Medium" FontSize="27" Background="{x:Null}"/>
        <StackPanel x:Name="mg_panel" Margin="55,198,76,66" Grid.ColumnSpan="2">
            <ListBox x:Name="scoll" Height="520"/>
        </StackPanel>

        <ProgressBar adonisExtensions:ProgressBarExtension.Content="Loading"
             Minimum="0"
             Value="70"
             Maximum="100"
             IsIndeterminate="True"
             Padding="0, 2" Margin="398,111,220,652" ValueChanged="ProgressBar_ValueChanged"/>
        <Button x:Name="select" Click="select_file_Click" Content="Select File" HorizontalAlignment="Left" Margin="398,110,0,0" VerticalAlignment="Top" Height="22" Width="152"/>

    </Grid>
</Window>
